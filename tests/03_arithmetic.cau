assert(0 + 0 == 0);
assert(1 + 2 == 3);

assert(2 * 3 == 6);
assert(2 * 0 == 0);

assert(10 / 5 == 2);
assert(13 / 5 == 2);

assert(5 - 2 == 3);
assert(5 - -2 == 7);
assert(5 - - 2 == 7);
assert(-5 - - 2 == 2);
assert(- -5 - - 2 == 7);

assert(0 % 3 == 0);
assert(1 % 3 == 1);
assert(2 % 3 == 2);
assert(3 % 3 == 0);

let text = "123";
assert(text + text == "123123");
assert(text + "456" == "123456");
assert(text * 0 == "");
assert(text * 1 == text);
assert(text * 3 == "123123123");
assert(text * 3 + text == text * 2 + text * 2);

let integer = 10;
assert(integer + integer == 20);
assert(integer + integer + integer == integer * 3);
assert(integer >= 10);
assert(integer <= 10);
assert(integer > 5);
assert(integer >= 5);
assert(integer < 20);
assert(integer <= 20);
assert(integer != 0);

let decimal = 100.0;
assert(integer == 10.0);
assert(integer >= 10.0);
assert(integer <= 10.0);
assert(integer > 5.0);
assert(integer >= 5.0);
assert(integer < 20.0);
assert(integer <= 20.0);
assert(integer != 0.0);

let character = '1';
assert(character == '1');
assert(character >= '1');
assert(character <= '1');
assert(character > '0');
assert(character >= '0');
assert(character < '2');
assert(character <= '2');
assert(character != '0');
